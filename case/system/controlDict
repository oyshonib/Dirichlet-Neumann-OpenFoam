FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//libs ( "libgroovyBC.so" ) ;

//application     waveFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10; //8;

deltaT          1; //0.1;

writeControl    timeStep; //runTime;  //timeStep;

writeInterval   1; //0.1;  //0.02;  //50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;  //yes;


